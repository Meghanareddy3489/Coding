class Solution {
    public int maxSubArray(int[] nums) {
        int n = nums.length;
        int res =0;
        for(int i=0;i<n;i++){
            for(int j=i;j<n;j++){
                for(int k =i;k<=j;k++){
                    sum+=nums[k];
                }
             res = Math.max(res,sum);
            }
        }
    }
}
